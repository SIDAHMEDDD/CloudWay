<div class="profile">
  <img src="/images/avatar.jpg" />
  <p id="name">{{userr.fname}}&nbsp;{{userr.sname}}</p>
  <p id="username">As {{userr.username}}</p>
</div>
{{#each eventss}}
  <div class="event">
    <div class="title">
      <img src="/images/avatar.jpg" />
      <h4>{{this.uploader}}</h4>
    </div>
    <div class="content">
      <p>{{this.content}}</p>
    </div>
    <hr width="95%" />
    <div class="controlls">
      <h4>{{this.likes}} Person likes this</h4>
      <a id="share" href="#"><i class="fa fa-share-alt-square" aria-hidden="true"></i></a>
      <a id="comment" href="#"><i class="fa fa-comment" aria-hidden="true"></i></a>
      <a id="like" href="#"><i class="fa fa-heart" aria-hidden="true"></i></a>
    </div>
  </div>
{{/each}}
